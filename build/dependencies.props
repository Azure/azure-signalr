<Project>
  <PropertyGroup>
    <MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
  </PropertyGroup>
  <PropertyGroup Label="Package Versions">
    <!-- Azure SignalR -->
    <MessagePackPackageVersion>1.7.3.4</MessagePackPackageVersion>
    <MicrosoftAspNetCoreHttpConnectionsClientPackageVersion>1.0.0-rc1-30677</MicrosoftAspNetCoreHttpConnectionsClientPackageVersion>
    <MicrosoftAspNetCoreSignalRPackageVersion>1.0.0-rc1-30677</MicrosoftAspNetCoreSignalRPackageVersion>
    <MicrosoftAspNetCoreSignalRProtocolsMessagePackPackageVersion>1.0.0-rc1-30677</MicrosoftAspNetCoreSignalRProtocolsMessagePackPackageVersion>
    <MicrosoftIdentitiyModelClientsActiveDirectoryPackageVersion>3.19.1</MicrosoftIdentitiyModelClientsActiveDirectoryPackageVersion>
    <MicrosoftAzureKeyVaultPackageVersion>2.3.2</MicrosoftAzureKeyVaultPackageVersion>
    <SystemBuffersPackageVersion>4.5.0-rc1-26423-06</SystemBuffersPackageVersion>
    <SystemMemoryPackageVersion>4.5.0-rc1-26423-06</SystemMemoryPackageVersion>
    <SystemIdentityModelTokensJwtPackageVersion>5.2.1</SystemIdentityModelTokensJwtPackageVersion>
    <MicrosoftExtensionsDependencyInjectionPackageVersion>2.1.0-rc1-30677</MicrosoftExtensionsDependencyInjectionPackageVersion>
    <MicrosoftExtensionsLoggingPackageVersion>2.1.0-rc1-30677</MicrosoftExtensionsLoggingPackageVersion>

    <!-- Samples -->
    <MicrosoftAspNetCorePackageVersion>2.1.0-rc1-30677</MicrosoftAspNetCorePackageVersion>
    <NewtonsoftJsonPackageVersion>11.0.2</NewtonsoftJsonPackageVersion>

    <!--Testing -->
    <MicrosoftNETTestSdkPackageVersion>15.6.1</MicrosoftNETTestSdkPackageVersion>
    <MoqPackageVersion>4.7.49</MoqPackageVersion>
    <XunitPackageVersion>2.3.1</XunitPackageVersion>
    <XunitRunnerVisualStudioPackageVersion>2.4.0-beta.1.build3945</XunitRunnerVisualStudioPackageVersion>
  </PropertyGroup>
  <Import Project="$(DotNetPackageVersionPropsPath)" Condition=" '$(DotNetPackageVersionPropsPath)' != '' " />
</Project>
