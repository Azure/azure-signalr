          },
          {
            "in": "query",
            "name": "excluded",
            "description": "Exclude these connectionIds when closing the connections in the hub.",
            "type": "array",
            "items": {
              "type": "string"
            },
            "collectionFormat": "multi"
          },
          {
            "in": "query",
            "name": "reason",
            "description": "The reason closing the client connections.",
